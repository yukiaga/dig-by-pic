<%= form_for [artwork] do |f| %>
  <%= f.label :title, "title" %>
  <%= f.text_field :title, placeholder: 'text a title' %>

  <%= f.label :image, "artwork" %>
  <%= f.text_field :image %>

  <%= f.label :link, "link" %>
  <%= f.text_field :link, placeholder: 'text a spotify link' %>

  <%= f.label :artist, "artist" %>
  <%= f.text_field :artist, placeholder: 'text a artist name' %>

  <%= f.submit 'Post' %>
<% end %>
